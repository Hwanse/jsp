<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="uri">
	
	<!-- 모든 URI 리스트 조회 -->
	<select id="getAllUriMapping" resultType="uriVO">
		SELECT	*
		FROM	URIMAPPING
	</select>

	<!-- 특정 URI 조회 -->
	<select id="getUriMapping" parameterType="string" resultType="uriVO">
		SELECT	*
		FROM	URIMAPPING
		WHERE	URI = #{uri}
	</select>

</mapper>